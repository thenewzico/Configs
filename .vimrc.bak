execute pathogen#infect()
set nocompatible
set autoindent
filetype on
filetype plugin on
filetype plugin indent on
set fileformats=unix
syntax enable
syntax on
set number
set incsearch
set hlsearch
set ignorecase
map N Nzz
map n nzz
set noerrorbells
set background=dark
colorscheme solarized
set t_Co=16
set encoding=utf-8
set laststatus=2
